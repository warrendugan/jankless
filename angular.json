{
  "version": 1,
  "cli": {
    "defaultCollection": "@nrwl/angular",
    "packageManager": "yarn"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "style": "css",
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "css"
    }
  },
  "projects": {
    "ui": {
      "projectType": "library",
      "root": "libs/ui",
      "sourceRoot": "libs/ui/src",
      "prefix": "jankless",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "options": {
            "tsConfig": "libs/ui/tsconfig.lib.json",
            "project": "libs/ui/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/ui/tsconfig.lib.prod.json"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/ui/src/**/*.ts",
              "libs/ui/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/ui"
          ],
          "options": {
            "jestConfig": "libs/ui/jest.config.js",
            "passWithNoTests": true
          }
        },
        "publish": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "command": "ng build ui --prod && cd dist/libs/ui/ && npm pack && npm publish --access public"
          }
        }
      }
    },
    "jankless": {
      "projectType": "application",
      "root": "apps/jankless",
      "sourceRoot": "apps/jankless/src",
      "prefix": "jankless",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/jankless",
            "index": "apps/jankless/src/index.html",
            "main": "apps/jankless/src/main.ts",
            "polyfills": "apps/jankless/src/polyfills.ts",
            "tsConfig": "apps/jankless/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/jankless/src/favicon.ico",
              "apps/jankless/src/assets"
            ],
            "styles": [
              "apps/jankless/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/jankless/src/environments/environment.ts",
                  "with": "apps/jankless/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "jankless:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "jankless:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "jankless:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/jankless/src/**/*.ts",
              "apps/jankless/src/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/jankless"
          ],
          "options": {
            "jestConfig": "apps/jankless/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "jankless-e2e": {
      "root": "apps/jankless-e2e",
      "sourceRoot": "apps/jankless-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/jankless-e2e/cypress.json",
            "tsConfig": "apps/jankless-e2e/tsconfig.e2e.json",
            "devServerTarget": "jankless:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "jankless:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/jankless-e2e/**/*.{js,ts}"
            ]
          }
        }
      }
    }
  },
  "defaultProject": "jankless"
}